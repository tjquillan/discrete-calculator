{
  "name": "discrete-calculator",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "format": "prettier --write src",
    "index": "cti ./src/components/*/",
    "build:grammar:proposition": "nearleyc src/util/parser/proposition/grammar.ne -o src/util/parser/proposition/grammar.ts",
    "build:grammar:set": "nearleyc src/util/parser/set/grammar.ne -o src/util/parser/set/grammar.ts",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write",
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.3",
    "@matejmazur/react-katex": "^3.1.3",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "clsx": "^1.1.1",
    "immutable": "4.0.0",
    "katex": "^0.15.3",
    "mathlive": "0.69.11",
    "nearley": "^2.20.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "react-table": "^7.7.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/moo": "^0.5.5",
    "@types/nearley": "^2.11.2",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.10",
    "create-ts-index": "^1.14.0",
    "husky": "^4",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.0",
    "react-scripts": "5.0.0",
    "typescript": "^4.6.2"
  }
}
